  width: 83%;
  height: 2.5em;
  border-bottom: 1px outset #F5F7F5;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  border: 2px solid orange;
  
}



.noti-list{
/* border-top-radius: 25px; border:5px outset #F5F7F5; */
  margin: 1.4em 0% 0% 0.2em;  width:28.4em;
 /* background-color:#B0DBA9;*/ height:2em; 
  border-bottom: none;

}

table.it-list a{text-decoration: none;}
table.it-list {width:100%;}
.main-post table.noti{margin: -3.2% 0% 2% 0.05%;border-radius:0px; border:0px; bottom: 0px;}
.main-post table.comment{position: absolute; bottom: -7.5%;}

table.noti{
  position: relative;
  border-radius: 25px; border:5px outset #F5F7F5;
  margin: -3.2% 0% 0% 0.05%; 
  height:12%; width:100%;
    font-size: 1em;

}

table tr{
  margin: 0% 0% 0% 0%; border-top-left-radius:10px; border-top-right-radius:25px;
  background-color:white;    font-size: 14px;
}


.noti .td-left{
  text-align: right;
  padding-right: 15%;
}

table tr th{
  text-align: center;  font-size: 12px;
  background-color:#B0DBA9; height:5em; 
}

.noti .top-first-th{
 border-top-left-radius:10px; 
}
.noti .top-last-th{
   border-top-right-radius:25px;
}
/**accept offer/request offer*/
.noti .bottom-first-th{border-bottom-left-radius:10px;cursor: pointer; cursor: hand;}
.noti .bottom-last-th{border-bottom-right-radius:25px; cursor: pointer; cursor: hand;}
.noti  h2{color:green; font-size: 2em; font-weight: bold;}
